{
    "name": "Muse Extension",
    "version": "1.0",
    "description": "Extension to browse the web using the Muse headset",
    "permissions": ["storage", "input"],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    }, 
    "commands": {
      "next-element": {
        "suggested_key": {
          "default": "Ctrl+Shift+Y",
          "mac": "Command+Shift+Y"
        },
        "description": "Next element"
      }
    },
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["contentScript.js"]
        }
      ],
    "manifest_version": 2
  }
  